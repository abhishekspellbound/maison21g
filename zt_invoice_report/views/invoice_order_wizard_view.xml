<odoo>


    <record model="ir.ui.view" id="invoice_order_report_wizard">
        <field name="name">invoice.order.xlsx</field>
        <field name="model">invoice.order.xlsx</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form >
                <group>
                    <group>
                        <field name="date_start"/>
                    </group>
                    <group>
                        <field name="date_end"/>
                    </group>
<!--                    <group>-->
<!--                        <field name="team_sales_id"/>-->
<!--                    </group>-->
                </group>
                <footer>
                    <button name="get_report" string="Generate Report" type="object" class="oe_highlight"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <act_window id="action_report_wizard_invoice_mg"
                name="Invoice Item Report"
                res_model="invoice.order.xlsx"
                view_mode="form"
                target="new"/>

    <menuitem action="action_report_wizard_invoice_mg"
              id="menu_account_mg_report_report" sequence="101"
              parent="account.account_reports_management_menu"/>



    <!--    CRM-->





</odoo>
