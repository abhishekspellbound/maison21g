<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sale_order_tree_view_inherit" model="ir.ui.view">
        <field name="name">sale.order.tree.view.inherit</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='order_lines']/field/tree/field[@name='product_uom_qty']" position='before'>
                <field name="available_qty"/>
            </xpath>
        </field>
    </record>

</odoo>
